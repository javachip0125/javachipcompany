<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="login">
        SELECT USER_ID
               USER_PW
          FROM JAVACHIP_MEMBER
    </entry>

    <entry key="DELETE_ATTENDANCE_INFO ">
        DELETE
        FROM ATTENDANCE_INFO
        WHERE JAVACHIP_CODE = ?
    </entry>

    <entry key="DELETE_JAVACHIP_MEMBER">
        DELETE
          FROM JAVACHIP_MEMBER
         WHERE JAVACHIP_CODE = ?
    </entry>

    <entry key="DELETE_WORK_SCHEDULE">
        DELETE
          FROM WORK_SCHEDULE
         WHERE JAVACHIP_CODE = ?
    </entry>

    <entry key="EMPLOYEE_INQUIRY">
        SELECT
               JAVACHIP_CODE,
               WORK_HOUR,
               REMAIN_VACATION,
               WORK_STATUS,
               ARRIVE_INFO,
               LEAVE_INFO,
               WORK_SCHEDULE

          FROM ATTENDANCE_INFO
         WHERE JAVACHIP_CODE = ?
    </entry>


</properties>