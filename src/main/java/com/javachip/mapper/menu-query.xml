<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="login">
        SELECT USER_ID
               USER_PW
          FROM JAVACHIP_MEMBER
    </entry>

    <entry key="DELETE_QUITTER ">
        DELETE
        FROM ATTENDANCE_INFO
        WHERE JAVACHIP_CODE = ?;
    </entry>

    <entry key="">
        DELETE
          FROM JAVACHIP_MEMBER
         WHERE JAVACHIP_CODE = ?;
    </entry>

    <entry key="">
        DELETE
          FROM WORK_SCHEDULE
         WHERE JAVACHIP_CODE = ?;
    </entry>

    <entry key="">
        SELECT
               JAVACHIP_CODE,
               POSITION,
               JAVACHIP_NAME
        FROM JAVACHIP_MEMBER;
    </entry>


</properties>